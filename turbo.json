{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "build/**",
        "dist/**"
      ]
    },
    "check": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "serve": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
